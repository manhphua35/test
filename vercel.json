{
  "version": 2,
  "builds": [
    {
      "src": "Money-Manager-React-master/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "Back-end/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
